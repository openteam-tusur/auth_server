<h2>Регистрация</h2>

<%= simple_form_for resource, :url => registration_path(resource_name) do |f| %>
  <%= f.input :surname %>
  <%= f.input :name %>
  <%= f.input :patronymic %>
  <%= f.input :email %>
  <%= f.input :password %>
  <%= f.input :password_confirmation %>
  <%= f.submit 'Зарегистрироваться' %>
<% end %>

<%= render "devise/shared/links" %>
